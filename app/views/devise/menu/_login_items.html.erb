<li>
  <% if user_signed_in? %>
    <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: 'btn' %>
  <% else %>
    <%= link_to "Sign In", new_user_session_path, class: 'btn' %>
  <% end %>
</li>
<li>
  <% if !user_signed_in? %>
    <%= link_to "Sign Up", new_user_registration_path, class: 'btn btn-primary' %>
  <% end %>
</li>
<% if user_signed_in? %>
    <li>
      <%= link_to 'Account',edit_user_registration_path, class: 'btn' %>
    </li>
    <li>
      <%= link_to 'Profile', user_profile_path(current_user.id), class: 'btn' %>
    </li>
<% end %>
